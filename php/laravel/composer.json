{
	"name": "demo",
	"require": {
		"php": ">=5.6",
		"laravel/framework": "5.2.*",
		"predis/predis": "1.1.*"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"repositories": [
		{
			"packagist": false
		},
		{
			"type": "composer",
			"url": "https://packagist.phpcomposer.com"
		}
	],
	"scripts": {
		"post-install-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postInstall",
			"php artisan.php optimize"
		],
		"post-update-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"php artisan.php optimize"
		]
	}
}
